<%- include('header')%>

 	<div class="container">

	  	<div class="row justify-content-center">
	  		<% db.Bvlgari.forEach(function(watch){ %>
				<div class="card text-white bg-dark text-center" style="width: 16rem; margin: 10px;">
					
					  <img class="card-img-top img-fluid" src="<%= watch.img %>" alt="Card image cap">
					  <div class="card-body">
					  <h5 class="card-title"><%= watch.model %></h5>
					  <p class="card-text"><%= watch.price %></p>
					
				    <a href="#" class="btn btn-secondary">Buy Now</a>
				  </div>
				</div>
			<% })%>
		</div>
		
 	</div>

<%- include('footer')%>